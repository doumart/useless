    "electron-builder": "^22.10.5",
    "electron-windows-store": "^2.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-jest-formatting": "^2.0.1",
