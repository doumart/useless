{
  "name": "uselessrobot",
  "productName": "UselessRobot",
  "version": "1.0.1",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start:renderer": "webpack serve --mode=development --config=webpack.renderer.config.js",
    "start:main": "webpack --mode=development --config=webpack.main.config.js && electron .webpack/main",
    "start": "concurrently \"yarn start:renderer\" \"yarn start:main\"",
    "build:renderer": "webpack --mode=production --config=webpack.renderer.config.js",
    "build:main": "webpack --mode=production --config=webpack.main.config.js",
    "build": "yarn build:renderer && yarn build:main && electron-builder",
    "test:ava": "ava",
    "test:jest": "jest",
    "publish": "yarn build:renderer && electron-builder -p always",
    "lint": "eslint --ext .ts,.tsx,.js .",
    "lint:css": "npx stylelint \"**/*.(scss|css)\""
  },
  "keywords": [],
  "author": {
    "name": "Spaceparanoid",
    "email": "doum_art@hotmail.com"
  },
  "license": "MIT",
